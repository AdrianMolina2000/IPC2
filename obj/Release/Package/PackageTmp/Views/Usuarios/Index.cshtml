@model List<IPC2.Models.ViewModels.ListTablaViewModel>
@{
    ViewBag.Title = "Datos";
    Layout = null;
}
<!DOCTYPE html>
<html> 
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="icon" type="image/png" href="../Images/icon.png">
    <title>@ViewBag.Title - Mi aplicación ASP.NET</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body class="container body-content">
    <div class="row">
        <div class="col-md-push-12">
            <table class="table-condensed table-hover table-bordered table-striped">
                <tr>
                    <th>NickName</th>
                    <th>Primer Nombre</th>
                    <th>Segundo Nombre</th>
                    <th>Primer Apellido</th>
                    <th>Segundo Apellido</th>
                    <th>Fecha de Nacimiento</th>
                    <th>Pais</th>
                    <th>Contraseña</th>
                    <th>Email</th>
                </tr>
                @foreach (var oElemento in Model)
                {
                    <tr>
                        <td>@oElemento.Nickname</td>
                        <td>@oElemento.PrimerNombre</td>
                        <td>@oElemento.SegundoNombre</td>
                        <td>@oElemento.PrimerApellido</td>
                        <td>@oElemento.SegundoApellido</td>
                        <td>@oElemento.FechaNacimiento</td>
                        <td>@oElemento.Pais</td>
                        <td>@oElemento.Password</td>
                        <td>@oElemento.Email</td>

                    </tr>
                }
            </table>

        </div>

    </div>
</body>
</html>